declare class CEventTarget {
    constructor();
}

declare class CNode extends CEventTarget {
    el?: Node;
    constructor();
    textContent(value: string): this;
}

declare class CElement<T> extends CNode {
    el?: Element;
    constructor();
    setAttribute(name: string, value: string): this;
    className(value: string): this;
    id(value: string): this;
    innerHtml(value: string): this;
    outerHtml(value: string): this;
    scrollLeft(value: number): this;
    scrollTop(value: number): this;
    slot(value: string): this;
    children(...value: (Node | string)[]): this;
    get(): T;
}

export { CElement, CEventTarget, CNode };
